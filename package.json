{
  "name": "rollup-plugin-debug",
  "version": "0.1.0",
  "description": "Rollup plugin for debugging plugins and builds",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.mjs",
  "scripts": {
    "build:config": "tsc --project config/tsconfig.json",
    "prebuild:devtools": "npm run build:config",
    "build:devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "npm run build",
    "test": "jest",
    "prebuild": "npm run build:devtools",
    "build": "rollup -c",
    "clean": "rm -rf .rpt2_cache build lib devtools/build devtools/lib config/build config/lib",
    "prepublishOnly": "npm run clean && NODE_ENV=production npm run build"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BobKerns/rollup-plugin-debug.git"
  },
  "keywords": [
    "template",
    "node",
    "typescript",
    "rollup"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/rollup-plugin-debug/issues"
  },
  "homepage": "https://github.com/BobKerns/rollup-plugin-debug#readme",
  "dependencies": {
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "wcsize": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/ramda": "^0.26.36",
    "jest": "^24.9.0",
    "ramda": "^0.26.1",
    "rollup": "^1.27.8",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-visualizer": "^1.1.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  }
}
